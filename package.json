  "scripts": {
    "start": "node --max_old_space_size=3072 node_modules/@angular/cli/bin/ng serve",
    "start-ip": "npm run start -- --host 0.0.0.0",
    "build": "node --max_old_space_size=3072 node_modules/@angular/cli/bin/ng build",
    "build:prod": "npm run build -- --configuration=prod",
    "build:staging": "npm run build -- --configuration=staging",
    "build:dev": "npm run build -- --configuration=dev",
    "build:cordova-prod": "npm run build -- --configuration=cordova-prod",
    "build:cordova-staging": "npm run build -- --configuration=cordova-staging",
    "build:cordova-dev": "npm run build -- --configuration=cordova-dev",
    "test": "node --expose-gc ./node_modules/.bin/ng test --coverage --log-heap-usage",
    "test:watch": "ng test --watch-all",
    "test:tdd": "ng test --config-path ./jest/jest-tdd.config.js --watch --cache --only-changed",
    "lint": "mynd-lint",
    "lint:fix": "mynd-lint --fix",
    "lint:styl": "mynd-lint-styl",
    "prepare": "husky install",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && patch-package"
